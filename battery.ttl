# Manufacturing process KPIs
# CQ_MAN_8.1 KPIs for LiB man. process: Scrap (rate), Production cost (per unit)
# CQ_MAN_8.2 KPIs for RFB man. process: Production performance, Production cost, Throughput failure rate

lbco:lib_manufacturing_process a owl:Class ;
     rdfs:label "LiB manufacturing process" ;
     rdfs:subClassOf lbco:manufacturing_process .

lbco:rfb_manufacturing_process a owl:Class ;
     rdfs:label "RFB manufacturing process" ;
     rdfs:subClassOf lbco:manufacturing_process .

lbco:scrap_rate a owl:Class ;
     rdfs:label "Scrap rate" ;
     rdfs:subClassOf lbco:property .

lbco:production_performance a owl:Class ;
     rdfs:label "Production performance" ;
     rdfs:subClassOf lbco:property ;
     rdfs:comment "The performance/efficiency of production."^^xs:string . # TODO: quantify better

lbco:production_cost_per_unit a owl:Class ; # duplicated in manufacturing module
     rdfs:label "Production cost per unit" ;
     rdfs:subClassOf lbco:property . # TOSEE: annotation?

lbco:throughput_failure_rate a owl:Class ;
     rdfs:label "Throughput failure rate" ;
     rdfs:subClassOf lbco:property . # TOSEE: annotation?

lbco:lib_manufacturing_process
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_process_kpi ;
	owl:someValuesFrom lbco:scrap_rate] ;
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_process_kpi ;
	owl:someValuesFrom lbco:production_cost_per_unit] .

lbco:rfb_manufacturing_process 
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_process_kpi ;
	owl:someValuesFrom lbco:production_cost_per_unit] ;
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_process_kpi ;
	owl:someValuesFrom lbco:throughput_failure_rate] ;
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_process_kpi ;
	owl:someValuesFrom lbco:production_performance] .

lbco:lib_manufacturing_process_step a owl:Class ;
	rdfs:subClassOf lbco:process_step, lbco:lib_manufacturing_process .

lbco:rfb_manufacturing_process_step a owl:Class ;
	rdfs:subClassOf lbco:process_step, lbco:rfb_manufacturing_process .

