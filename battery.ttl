lbco:DIFFUSION_MECHANICAL_EQUATION_MODEL_FOR_CALENDERING
	a lbco:physics_based_model ;
	a lbco:calendering_model ;
	a lbco:lib_model ;
	lbco:has_granularity_level osmo:CONTINUUM .

lbco:MD_MODEL_FOR_RFB_ELECTROLYTE_TRANSPORT_PROPERTIES 
	a lbco:physics_based_model ;
	a lbco:rfb_model ;
	a lbco:electrolyte_model ; 
	lbco:has_granularity_level osmo:ATOMISTIC ;
	lbco:has_physical_equation_type osmo:INDIVIDUAL_OF_PE_TYPE_A.3 .

