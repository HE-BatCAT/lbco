     rdfs:label "LiB manufacturing process" ;
     rdfs:subClassOf mbco:process .

mbco:rfb_manufacturing_process a owl:Class ;
     rdfs:label "RFB manufacturing process" ;
     rdfs:subClassOf mbco:process .

mbco:scrap_rate a owl:Class ;
