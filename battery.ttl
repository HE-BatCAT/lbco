### Classes ###

mbco:lithium_ion_battery a owl:Class ;
     rdfs:label "Lithium-ion battery (LiB)" ;
     rdfs:subClassOf mbco:physical_object ;
     rdfs:subClassOf mbco:whole_product .
     
mbco:redox_flow_battery a owl:Class ;
     rdfs:label "Redox-flow battery (RFB)" ;
     rdfs:subClassOf mbco:physical_object ;
     rdfs:subClassOf mbco:whole_product .

# LiB main components. based on CQ_CRO_10 (from Excel sheet) NB: not all entries included. Coating added (from MIRO)
# (CQ_CRO_10: Active material, Binder, Percolant, slurry solvent, slurry, coated electrode, calendered electrode, electrode, separator, electrolyte, collector, battery cell, casing)
# NB: I am not adding slurry, slurry solvent and coated electrode as I think they belong elsewhere, maybe intermediate products?
mbco:lithium_ion_battery
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty mbco:has_part ;
     owl:someValuesFrom mbco:active_material] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty mbco:has_part ;
     owl:someValuesFrom mbco:binder] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty mbco:has_part ;
     owl:someValuesFrom mbco:percolant] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty mbco:has_part ;
     owl:someValuesFrom mbco:electrode] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty mbco:has_part ;
     owl:someValuesFrom mbco:separator] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty mbco:has_part ;
     owl:someValuesFrom mbco:electrolyte] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty mbco:has_part ;
     owl:someValuesFrom mbco:collector] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty mbco:has_part ;
     owl:someValuesFrom mbco:casing] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty mbco:has_part ;
     owl:someValuesFrom mbco:coating] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty mbco:has_part ;
     owl:someValuesFrom mbco:battery_cell] .

# CQ_CRO_4: RFB main components
mbco:redox_flow_battery
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_part ;
	owl:someValuesFrom mbco:bipolar_plate] ;
        rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_part ;
	owl:someValuesFrom mbco:membrane] ;
        rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_part ;
	owl:someValuesFrom mbco:carbon_felt] ;
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_part ;
	owl:someValuesFrom mbco:electrolyte] .

# CQ_MAN_9.1: LiB product KPIs.
mbco:lithium_ion_battery
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_product_kpi ;
	owl:someValuesFrom mbco:full_cell_initial_performance] ; # TOSEE: initial_performance -> full_cell_*: OK?
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_product_kpi ;
	owl:someValuesFrom mbco:full_cell_ageing] ; # TOSEE: keep it???
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_product_kpi ;
	owl:someValuesFrom mbco:full_cell_safety] . # TOSEE: keep it???

# CQ_MAN_9.2: RFB product KPIs. TOSEE: add "efficiency"? (removed currently)
mbco:redox_flow_battery
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_product_kpi ;
	owl:someValuesFrom mbco:round_trip_efficiency] ;
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_product_kpi ;
	owl:someValuesFrom mbco:full_cell_capacity] ; # TOSEE: capacity -> full_cell_capacity: OK?
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_product_kpi ;
	owl:someValuesFrom mbco:full_cell_durability] . # TOSEE: durability -> full_cell_durability: OK?

