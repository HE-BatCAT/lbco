### Classes ###

lbco:lithium_ion_battery a owl:Class ;
     rdfs:label "Lithium-ion battery (LiB)" ;
     rdfs:subClassOf lbco:physical_object ;
     rdfs:subClassOf lbco:whole_product ;
     rdfs:subClassOf lbco:battery .    
     
lbco:redox_flow_battery a owl:Class ;
     rdfs:label "Redox-flow battery (RFB)" ;
     rdfs:subClassOf lbco:physical_object ;
     rdfs:subClassOf lbco:whole_product ;
     rdfs:subClassOf lbco:battery .    

# LiB main components. based on CQ_CRO_10 (from Excel sheet) NB: not all entries included. Coating added (from MIRO)
# (CQ_CRO_10: Active material, Binder, Percolant, slurry solvent, slurry, coated electrode, calendered electrode, electrode, separator, electrolyte, collector, battery cell, casing)
# NB: I am not adding slurry and coated electrode as I think they belong elsewhere, maybe intermediate products?
lbco:lithium_ion_battery
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty lbco:has_part ;
     owl:someValuesFrom lbco:active_material] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty lbco:has_part ;
     owl:someValuesFrom lbco:binder] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty lbco:has_part ;
     owl:someValuesFrom lbco:percolant] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty lbco:has_part ;
     owl:someValuesFrom lbco:slurry_solvent] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty lbco:has_part ;
     owl:someValuesFrom lbco:electrode] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty lbco:has_part ;
     owl:someValuesFrom lbco:separator] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty lbco:has_part ;
     owl:someValuesFrom lbco:electrolyte] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty lbco:has_part ;
     owl:someValuesFrom lbco:collector] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty lbco:has_part ;
     owl:someValuesFrom lbco:casing] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty lbco:has_part ;
     owl:someValuesFrom lbco:coating_layer] ;
     rdfs:subClassOf [a owl:Restriction ;
     owl:onProperty lbco:has_part ;
     owl:someValuesFrom lbco:full_cell] . # battery_cell -> full cell . OK? or better to rename it?

# CQ_CRO_4: RFB main components
lbco:redox_flow_battery
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_part ;
	owl:someValuesFrom lbco:bipolar_plate] ;
        rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_part ;
	owl:someValuesFrom lbco:membrane] ;
        rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_part ;
	owl:someValuesFrom lbco:carbon_felt] ;
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_part ;
	owl:someValuesFrom lbco:electrolyte] .

# CQ_MAN_9.1: LiB product KPIs.
lbco:lithium_ion_battery
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_product_kpi ;
	owl:someValuesFrom lbco:full_cell_initial_performance] ; # TOSEE: initial_performance -> full_cell_*: OK?
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_product_kpi ;
	owl:someValuesFrom lbco:full_cell_ageing] ; # TOSEE: keep it???
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_product_kpi ;
	owl:someValuesFrom lbco:full_cell_safety] . # TOSEE: keep it???

# CQ_MAN_9.2: RFB product KPIs. TOSEE: add "efficiency"? (removed currently)
lbco:redox_flow_battery
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_product_kpi ;
	owl:someValuesFrom lbco:round_trip_efficiency] ;
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_product_kpi ;
	owl:someValuesFrom lbco:full_cell_capacity] ; # TOSEE: capacity -> full_cell_capacity: OK?
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_product_kpi ;
	owl:someValuesFrom lbco:full_cell_durability] . # TOSEE: durability -> full_cell_durability: OK?

# From CQ_CRO_9.
lbco:rfb_length_scale a owl:Class ;
    rdfs:label "RFB length scale" ;
    owl:oneOf (lbco:MOLECULAR_SCALE lbco:FIBER_SCALE lbco:PORE_SCALE lbco:CELL_SCALE lbco:STACK_SCALE lbco:BATTERY_SYSTEM_SCALE lbco:ENERGY_SYSTEM_SCALE) ;
    rdfs:subClassOf lbco:length_scale ;
    rdfs:comment"""length and time scale can be used to annotate and classify a model, software or digital twin,
to specify which one it addresses"""^^xs:string .

lbco:lib_length_scale a owl:Class ;
    rdfs:label "LIB length scale" ;
    owl:oneOf (lbco:MOLECULAR_SCALE lbco:CELL_SCALE) ; # TODO: Check
    rdfs:subClassOf lbco:length_scale ;
    rdfs:comment"""length and time scale can be used to annotate and classify a model, software or digital twin,
to specify which one it addresses"""^^xs:string .

