lbco:lib_model a owl:Class ;
     owl:equivalentClass [a owl:Restriction ; owl:onProperty lbco:is_model_for ;
     owl:someValuesFrom lbco:lithium_ion_battery] .

lbco:rfb_model a owl:Class ;
     owl:equivalentClass [a owl:Restriction ; owl:onProperty lbco:is_model_for ;
     owl:someValuesFrom lbco:redox_flow_battery] .

lbco:sei_formation_process a owl:Class ;
	rdfs:subClassOf lbco:process .

lbco:sei_formation_model a owl:Class ;
	rdfs:label "SEI formation model"^^xs:string ;
    	owl:equivalentClass [a owl:Restriction ;
                   owl:onProperty lbco:is_model_for ;
                   owl:someValuesFrom lbco:sei_formation_process] .

lbco:KOHN_SHAM_SEI_FORMATION_MODEL a lbco:physics_based_model ;
		    a lbco:sei_formation_model ;
		    a lbco:lib_model ;
		    lbco:has_granularity_level osmo:ELECTRONIC ;
		    lbco:has_physical_equation_type osmo:INDIVIDUAL_OF_PE_TYPE_EL.1 .

lbco:electrolyte_model a owl:Class ;
	rdfs:label "Electrolyte model"^^xs:string ;
    	owl:equivalentClass [a owl:Restriction ;
                   owl:onProperty lbco:is_model_for ;
                   owl:someValuesFrom lbco:electrolyte] .

lbco:DPD_ELECTROLYTE_MODEL a lbco:physics_based_model ;
		    a lbco:electrolyte_model ;
		    lbco:has_granularity_level osmo:MESOSCOPIC ;
		    lbco:has_physical_equation_type osmo:INDIVIDUAL_OF_PE_TYPE_M.3 .

lbco:full_cell_model a owl:Class ;
	rdfs:label "Full cell model"^^xs:string ;
    	owl:equivalentClass [a owl:Restriction ;
                   owl:onProperty lbco:is_model_for ;
                   owl:someValuesFrom lbco:full_cell] .

lbco:P3D_CELL_MODEL a lbco:physics_based_model ;
		    a lbco:full_cell_model ;
		    a lbco:lib_model ;
		    lbco:has_granularity_level osmo:CONTINUUM ;
		    rdfs:comment "Pseudo 3 dimensional (P3D) cell model"^^xs:string .

lbco:P2D_CELL_MODEL a lbco:physics_based_model ;
		    a lbco:full_cell_model ;
		    a lbco:lib_model ;
		    lbco:has_granularity_level osmo:CONTINUUM ;
		    rdfs:comment "Pseudo 2 dimensional (P3D) cell model"^^xs:string .

lbco:SP_CIRCUIT_CELL_MODEL a lbco:physics_based_model ;
		    a lbco:full_cell_model ;
		    a lbco:lib_model ;
		    lbco:has_granularity_level osmo:CONTINUUM .

lbco:electrode_model a owl:Class ;
	rdfs:label "Electrode model"^^xs:string ;
	owl:equivalentClass [a owl:Restriction ;
                   owl:onProperty lbco:is_model_for ;
                   owl:someValuesFrom lbco:electrode] .

lbco:4D_RESOLVED_PHASE_FIELD_ELECTRODE_MODEL a lbco:physics_based_model ;
		    a lbco:electrode_model ;
		    a lbco:lib_model ;
		    lbco:has_granularity_level osmo:CONTINUUM .

