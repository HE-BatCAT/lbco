        owl:onProperty lbco:has_input ;
	owl:someValuesFrom lbco:binder] ;
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_input ;
	owl:someValuesFrom lbco:percolant] ;
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty lbco:has_input ;
