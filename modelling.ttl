### Classes ###

mbco:model_parametrization a owl:Class ;
    rdfs:label "Model parametrization" ;
    skos:altLabel "Model parameters" ; 
    rdfs:comment"Set of model parameters"^^xs:string ;   
    rdfs:subClassOf mbco:data .

mbco:simulation a owl:Class ;
    rdfs:label "Simulation" ;
    rdfs:subClassOf mbco:process ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_input ;
                   owl:someValuesFrom mbco:data] ;      
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_output ;
                   owl:someValuesFrom mbco:simulation_result] ;      
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_input ;
                   owl:someValuesFrom mbco:physical_quantity] ;      
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_output ;
                   owl:someValuesFrom mbco:physical_quantity] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_participant ;
                   owl:someValuesFrom mbco:software] .

mbco:simulation_result a owl:Class ;
    rdfs:label "Simulation result" ;
    rdfs:subClassOf mbco:data .

mbco:computational_modelling_method a owl:Class ;
    rdfs:label "Computational modelling method" ;
    rdfs:subClassOf mbco:method .
    
mbco:digital_twinning a owl:Class ; # TOSEE: OK?
    rdfs:label "Digital twinning" ;
    rdfs:subClassOf mbco:computational_modelling_method .

mbco:model_validation_process a owl:Class ;
    rdfs:label "Model validation process" ;
    rdfs:subClassOf mbco:process ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_input ;
                   owl:someValuesFrom mbco:model_parametrization] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_input ;
                   owl:someValuesFrom mbco:data] ;      # e.g., experimental data
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_output ;
                   owl:someValuesFrom mbco:statement] . # on the model validity
		   
mbco:model_parametrization_process a owl:Class ;
    rdfs:label "Model parametrization process" ;
    rdfs:subClassOf mbco:process ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_output ;
                   owl:someValuesFrom mbco:model_parametrization] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_input ;
                   owl:someValuesFrom mbco:data] . # e.g., experimental data

# From CQ_CRO_3
mbco:time_scale a owl:Class ;
    owl:oneOf (mbco:INDUSTRIAL_SAFETY_TIME_SCALE mbco:INTERMEDIATE_TIME_SCALE mbco:PLANNING_TIME_SCALE) ;
    rdfs:subClassOf mbco:annotation ;
    rdfs:comment"""length and time scale can be used to annotate and classify a model, software or digital twin,
 to specify which one it addresses"""^^xs:string .

# From CQ_CRO_9. Move to battery module later?
mbco:rfb_length_scale a owl:Class ;
    owl:oneOf (mbco:MOLECULAR_SCALE mbco:FIBER_SCALE mbco:PORE_SCALE mbco:CELL_SCALE mbco:STACK_SCALE mbco:BATTERY_SYSTEM_SCALE mbco:ENERGY_SYSTEM_SCALE) ;
    rdfs:subClassOf mbco:annotation ;
    rdfs:comment"""length and time scale can be used to annotate and classify a model, software or digital twin,
to specify which one it addresses"""^^xs:string .

mbco:lib_length_scale a owl:Class ;
    owl:oneOf (mbco:MOLECULAR_SCALE mbco:CELL_SCALE) ; # TODO: Check
    rdfs:subClassOf mbco:annotation ;
    rdfs:comment"""length and time scale can be used to annotate and classify a model, software or digital twin,
to specify which one it addresses"""^^xs:string .

mbco:physical_quantity a owl:Class ;
    rdfs:label "Physical quantity" ;
    rdfs:subClassOf mbco:property .

# CQ_SIM_9
mbco:model_reproducibility_method a owl:Class ; 
    rdfs:label "Model reproducibility method" ;
    rdfs:comment "Methods to evaluate the robustness of models and modelling data"^^xs:string ;
    rdfs:subClassOf mbco:data_quality_evaluation_method ; #TOSEE: OK?
    rdfs:subClassOf mbco:method .

mbco:VARYING_MODELLING_APPROACH a mbco:model_reproducibility_method ;
    rdfs:seeAlso """E.g., see S. Stephan et al, Vaporâˆ’Liquid Interface of the Lennard-Jones Truncated and Shifted Fluid: Comparison of Molecular Simulation, Density Gradient Theory, and Density Functional Theory, J.  Phys. Chem. C 2018 122 (43),
    4705-24715, DOI: 10.1021/acs.jpcc.8b06332 """^^xs:string .

mbco:VARYING_NUMERICAL_METHOD a mbco:model_reproducibility_method .

mbco:VARYING_MODELLING_SOFTWARE a mbco:model_reproducibility_method ;
    rdfs:seeAlso """E.g., see M. Schappals et al., Round Robin Study: Molecular Simulation of Thermodynamic Properties from Models with Internal Degrees of Freedom,
    J. Chem. Theory Comput. 2017 13 (9), 4270-4280, DOI: 10.1021/acs.jctc.7b00489"""^^xs:string .
    
### Object Properties ###

mbco:implements a owl:ObjectProperty ; # TOSEE: use the same property for plan implementation into process???
    rdfs:label "implements" ;
    rdfs:domain mbco:software ; 
    rdfs:range mbco:method . 

#mbco:is_model_for_system a owl:ObjectProperty ; 
#    rdfs:label "is model for system" ;
#    rdfs:domain mbco:model_parametrization . 

mbco:is_model_for_system_containing_material a owl:ObjectProperty ; 
    rdfs:label "is model for system containing material" ;
    rdfs:domain mbco:model_parametrization ;
    rdfs:range mbco:material .

mbco:has_model_type a owl:ObjectProperty ; 
    rdfs:label "has model type" ;
    rdfs:domain mbco:model_parametrization ;
    rdfs:range mbco:computational_modelling_method .

mbco:gives_modelling_value_for a owl:ObjectProperty ; #gives_prediction_for?
    rdfs:label "gives modelling value for" ;
    rdfs:domain mbco:computational_modelling_method ; #TOSEE: OK?
    rdfs:range mbco:physical_quantity ;
    rdfs:subPropertyOf mbco:gives_value_for .

mbco:gives_value_for a owl:ObjectProperty ; # quantifies?
    rdfs:label "gives value for" ;
    rdfs:domain mbco:method ; #TOSEE: OK?
    rdfs:range mbco:physical_quantity .

mbco:has_modelled_time_scale a owl:ObjectProperty ;
    rdfs:label "has modelled time scale" ;
#    rdfs:domain mbco:computational_modelling_method ;
    rdfs:range mbco:time_scale .

mbco:has_modelled_length_scale a owl:ObjectProperty ;
    rdfs:label "has modelled length scale" ;
#    rdfs:domain mbco:computational_modelling_method ;
    rdfs:range [owl:unionOf (mbco:lib_length_scale mbco:rfb_length_scale) ].

