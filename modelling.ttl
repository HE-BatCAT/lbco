### Classes ###

lbco:model_parametrization a owl:Class ; 
    rdfs:label "Model parametrization" ;
    skos:altLabel "Model parameters" ;
    skos:altLabel "Model" ;   # TO SEE: OK?
    rdfs:comment "Set of model parameters"^^xs:string ;
    rdfs:subClassOf lbco:data ;
    rdfs:subClassOf [a owl:Restriction ;
                  owl:onProperty lbco:has_action_type ;
                  owl:someValuesFrom lbco:model_action] ;
    rdfs:subClassOf [a owl:Restriction ;
                  owl:onProperty lbco:has_modelled_time_scale ;
                  owl:someValuesFrom lbco:time_scale] ;
    rdfs:subClassOf [a owl:Restriction ;
                  owl:onProperty lbco:has_modelled_length_scale ;
                  owl:someValuesFrom lbco:length_scale] .

lbco:simulation a owl:Class ;
    rdfs:label "Simulation" ;
    rdfs:subClassOf lbco:process ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_input ;
                   owl:someValuesFrom lbco:data] ;      
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_output ;
                   owl:someValuesFrom lbco:simulation_result] ;      
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_input ;
                   owl:someValuesFrom lbco:physical_quantity] ;      
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_output ;
                   owl:someValuesFrom lbco:physical_quantity] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_participant ;
                   owl:someValuesFrom lbco:software] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_input ;
                   owl:someValuesFrom lbco:model_parametrization] .

lbco:simulation_result a owl:Class ;
    rdfs:label "Simulation result" ;
    rdfs:subClassOf lbco:data .

lbco:computational_modelling_method a owl:Class ;
    rdfs:label "Computational modelling method" ;
    rdfs:subClassOf lbco:method .
    
lbco:digital_twinning a owl:Class ; # TOSEE: OK?
    rdfs:label "Digital twinning" ;
    rdfs:subClassOf lbco:computational_modelling_method .

# cf. EQ_SIM_3
lbco:model_validation_process a owl:Class ;
    rdfs:label "Model validation process" ;
    rdfs:subClassOf lbco:process ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_input ;
                   owl:someValuesFrom lbco:model_parametrization] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_input ;
                   owl:someValuesFrom lbco:data] ;      # e.g., experimental data
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_output ;
                   owl:someValuesFrom lbco:statement] ; # on the model validity
    rdfs:comment"""Its input contains data (e.g., from experiments, modelling methods at a different scale) and
    its output contains a statement about the model parametrization validity"""^^xs:string .

# cf. EQ_SIM_2
lbco:model_parametrization_process a owl:Class ;
    rdfs:label "Model parametrization process" ;
    rdfs:subClassOf lbco:process ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_output ;
                   owl:someValuesFrom lbco:model_parametrization] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_input ;
                   owl:someValuesFrom lbco:data] ; # e.g., experimental data
    rdfs:comment"""Its input contains data (e.g., from experiments) and its output a model parametrization"""^^xs:string .

# From CQ_CRO_3
lbco:time_scale a owl:Class ;
    rdfs:label "Time scale" ;
    owl:oneOf (lbco:INDUSTRIAL_SAFETY_TIME_SCALE lbco:INTERMEDIATE_TIME_SCALE lbco:PLANNING_TIME_SCALE) ;
    rdfs:subClassOf lbco:annotation ;
    rdfs:comment"""length and time scale can be used to annotate and classify a model, software or digital twin,
 to specify which one it addresses"""^^xs:string .

lbco:length_scale a owl:Class ;
    rdfs:label "Length scale" ;
    rdfs:subClassOf lbco:annotation ;
    rdfs:comment"""length and time scale can be used to annotate and classify a model, software or digital twin,
 to specify which one it addresses"""^^xs:string .

# lbco:physical_quantity a owl:Class ;
#     rdfs:label "Physical quantity" ;
#     rdfs:subClassOf lbco:property .

# CQ_SIM_9
lbco:model_reproducibility_method a owl:Class ; 
    rdfs:label "Model reproducibility method" ;
    rdfs:comment "Methods to evaluate the robustness of models and modelling data"^^xs:string ;
    rdfs:subClassOf lbco:data_quality_evaluation_method ; #TOSEE: OK?
    rdfs:subClassOf lbco:method .

lbco:VARYING_MODELLING_APPROACH a lbco:model_reproducibility_method ;
    rdfs:seeAlso """E.g., see S. Stephan et al, Vapor−Liquid Interface of the Lennard-Jones Truncated and Shifted Fluid: Comparison of Molecular Simulation, Density Gradient Theory, and Density Functional Theory, J.  Phys. Chem. C 2018 122 (43),
    4705-24715, DOI: 10.1021/acs.jpcc.8b06332 """^^xs:string .

lbco:VARYING_NUMERICAL_METHOD a lbco:model_reproducibility_method .

lbco:VARYING_MODELLING_SOFTWARE a lbco:model_reproducibility_method ;
    rdfs:seeAlso """E.g., see M. Schappals et al., Round Robin Study: Molecular Simulation of Thermodynamic Properties from Models with Internal Degrees of Freedom,
    J. Chem. Theory Comput. 2017 13 (9), 4270-4280, DOI: 10.1021/acs.jctc.7b00489"""^^xs:string .

lbco:model_action a owl:Class ;
    rdfs:label "Model action" ;
    rdfs:subClassOf lbco:process .    

lbco:PREDICT a lbco:model_action .
lbco:CLASSIFY a lbco:model_action . 
lbco:INTERPOLATE a lbco:model_action . 

lbco:data_driven_model a owl:Class ;
    rdfs:label "Data-driven model" ;
    rdfs:subClassOf lbco:model_parametrization ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:is_data_driven ;
                   owl:hasValue true] .      

lbco:physics_based_model a owl:Class ;
    rdfs:label "Physics-based model" ;
    rdfs:subClassOf lbco:model_parametrization ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:is_physics_based ;
                   owl:hasValue true] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_granularity_level ;
                   owl:allValuesFrom osmo:granularity_level] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_physical_equation_type ;
                   owl:allValuesFrom osmo:physical_equation_type] .

lbco:hybrid_model a owl:Class ;
    rdfs:label "Hybrid model" ;
    owl:equivalentClass [ owl:intersectionOf (lbco:physics_based_model lbco:data_driven_model ) ].

lbco:neural_network_model a owl:Class ;
    rdfs:label "Neural network model" ;
    rdfs:subClassOf lbco:data_driven_model .

### Object Properties ###

lbco:implements a owl:ObjectProperty ; # TOSEE: use the same property for plan implementation into process???
    rdfs:label "implements" ;
    rdfs:domain lbco:software ; 
    rdfs:range lbco:method . 

lbco:is_model_for a owl:ObjectProperty ; 
    rdfs:label "is model for" ;
    rdfs:domain lbco:model_parametrization ;
    rdfs:range [owl:unionOf (lbco:physical_object lbco:process)] . # TODO: compare with lbco:represents in twinning.ttl. Merge?

lbco:is_model_for_system_containing_material a owl:ObjectProperty ; 
    rdfs:label "is model for system containing material" ;
    rdfs:domain lbco:model_parametrization ;
    rdfs:range lbco:material .

lbco:is_model_for_system_containing_substance a owl:ObjectProperty ; 
    rdfs:label "is model for system containing substance" ;
    rdfs:domain lbco:model_parametrization ;
    rdfs:range lbco:substance ;
    rdfs:subPropertyOf lbco:is_model_for_system_containing_material .

lbco:has_model_type a owl:ObjectProperty ; 
    rdfs:label "has model type" ;
    rdfs:domain lbco:model_parametrization ;
    rdfs:range lbco:computational_modelling_method .

lbco:gives_modelling_value_for a owl:ObjectProperty ; #gives_prediction_for?
    rdfs:label "gives modelling value for" ;
    rdfs:domain lbco:computational_modelling_method ; #TOSEE: OK?
    rdfs:range lbco:physical_quantity ;
    rdfs:subPropertyOf lbco:gives_value_for .

lbco:gives_value_for a owl:ObjectProperty ; # quantifies?
    rdfs:label "gives value for" ;
    rdfs:domain lbco:method ; #TOSEE: OK?
    rdfs:range lbco:physical_quantity .

lbco:has_modelled_time_scale a owl:ObjectProperty ;
    rdfs:label "has modelled time scale" ;
#    rdfs:domain lbco:computational_modelling_method ;
    rdfs:range lbco:time_scale .

lbco:has_modelled_length_scale a owl:ObjectProperty ;
    rdfs:label "has modelled length scale" ;
#    rdfs:domain lbco:computational_modelling_method ;
    rdfs:range lbco:length_scale . # [owl:unionOf (lbco:lib_length_scale lbco:rfb_length_scale) ].

lbco:has_action_type a owl:ObjectProperty ; # TO DO: cf. usage for actuator and twin
    rdfs:label "has action type" ;
    rdfs:range lbco:process ;
    rdfs:comment "points to the action type an entity is built for or can perform"^^xs:string .

lbco:has_granularity_level a owl:ObjectProperty ;
     rdfs:label "has granularity level" ;
     rdfs:domain lbco:model_parametrization ;
     rdfs:range osmo:granularity_level ;
     rdfs:comment "points to the MODA granularity level"^^xs:string ;
     rdfs:comment "one of: Electronic, Atomistic, Mesoscopic (=from two atoms on), Continuum"^^xs:string ;
     rdfs:seeAlso """CEN-CENELEC Management Centre, Materials modelling: terminology, classification and
     metadata. CEN workshop agreement 17284, Brussels, Belgium (2018)"""^^xs:string.

lbco:has_physical_equation_type a owl:ObjectProperty ;
     rdfs:label "has physical equation type" ;
     rdfs:domain lbco:physics_based_model ;
     rdfs:range osmo:physical_equation_type ;
     rdfs:comment "points to one of the 24/25 RoMM/OSMO PE types"^^xs:string ;
     rdfs:comment "For example, PE type A.3: Molecular dynamics (atomistic)"^^xs:string ;
     rdfs:seeAlso "A.F. De Baas (ed.), What makes a material function? (EU Publications Office, Luxembourg, Let me compute the ways, 2017)"^^xs:string ;
     rdfs:seeAlso """M.T. Horsch, et al., Semantic interoperability and characterization of data provenance
     in computational molecular engineering. J. Chem. Eng. Data 65(3), 1313–1329 (2020)."""^^xs:string .

# Could add:
