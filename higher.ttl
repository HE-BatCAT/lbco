  rdfs:comment """The authors acknowledge funding from the European Union's Horizon Europe research and
innovation programme under grant agreement no. 101137725, Battery Cell Assembly Twin (BatCAT)."""^^xs:string .

lbco:agent a owl:Class ;
    rdfs:label "Agent" ;
    owl:disjointUnionOf (lbco:human_agent lbco:machine_agent) ;
    rdfs:comment "An entity that has the capability to act"^^xs:string .    

lbco:human_agent a owl:Class ;
    rdfs:label "Human agent" .

lbco:machine_agent a owl:Class ;
    rdfs:label "Machine agent" .

lbco:data a owl:Class ;
    rdfs:label "Data" .

lbco:material a owl:Class ;
    rdfs:label "Material" .

lbco:substance a owl:Class ;
    rdfs:label "Substance" ;
    rdfs:subClassOf lbco:material .

lbco:process a owl:Class ;
    rdfs:label "Process" ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_input ;
                   owl:someValuesFrom owl:Thing] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_output ;
                   owl:someValuesFrom owl:Thing] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_participant ; # TODO: clarify how different from input/output (cf. BVCO)??? E.g.: a machine+operator+sample are participants, input is certain data, output is other data.
                   owl:someValuesFrom owl:Thing] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_setting ;
                   owl:someValuesFrom owl:Thing] ;
    rdfs:comment "A series of actions, can be broken down in process steps of given order"^^xs:string ;
    rdfs:comment "Can be planned (not realized yet), or already realized"^^xs:string .		   

lbco:process_step a owl:Class ;
    rdfs:label "Process step" ;
    rdfs:subClassOf lbco:process ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty [owl:inverseOf lbco:has_part] ;
                   owl:someValuesFrom lbco:process] .

lbco:software a owl:Class ;
    rdfs:label "Software" .

lbco:physical_object a owl:Class ;
    rdfs:label "(Physical) Object" . 

lbco:sample a owl:Class ;
    rdfs:label "Sample" ;
    rdfs:subClassOf lbco:physical_object .

lbco:device a owl:Class ;
    rdfs:label "Device" ;
    rdfs:subClassOf lbco:physical_object ;
    rdfs:comment "A physical object made for a given functionality"^^xs:string .

lbco:computational_device a owl:Class ;
    rdfs:label "Computational device" ;
    rdfs:subClassOf lbco:device ;
    rdfs:comment "A device that can perform a (digital) computational action."^^xs:string ;
    rdfs:comment "E.g., a PC, a computational cluster."^^xs:string .

lbco:characterization_device a owl:Class ;
    rdfs:label "Characterization device" ;
    rdfs:subClassOf lbco:device ;
    rdfs:comment "A device that can perform a characterization action."^^xs:string ;
    rdfs:comment "E.g., a potentiostat."^^xs:string .

lbco:manufacturing_device a owl:Class ;
    rdfs:label "Manufacturing device" ;
    rdfs:subClassOf lbco:device ;
    rdfs:comment "A device that can perform a manufacturing action"^^xs:string ;
    rdfs:comment "E.g., a mixer, a calenderer."^^xs:string .    

lbco:method a owl:Class ;
    rdfs:label "Method" ;
    rdfs:comment """a procedure, technique, or planned way of doing something (WordReference Random House
    Learner's Dictionary of American English 2025)"""^^xs:string ;
    rdfs:comment "Could be a characterization method, a simulation method, a manufacturing method, etc."^^xs:string ;
    rdfs:subClassOf lbco:process . 

# lbco:compute_resource a owl:Class ;
#     rdfs:label "Compute resource" ; # we mean it as infrastrucure. Maybe should be renamed
#     rdfs:comment "A physical or virtual machine on which software can be run."^^xs:string ;
#     a owl:DeprecatedClass .

lbco:statement a owl:Class ;
    rdfs:label "Statement" . # Note: in EVMPO, statement is used for elementary communications. Here not, it is more general.   

lbco:role a owl:Class ;
    rdfs:label "Role" ;
    rdfs:comment "A non-rigid category (i.e., during its lifespan, an entity can change role)."^^xs:string ;
    rdfs:comment """For example: role of a human agent in an organization (employee/director), in a software repository (editor/owner); 
    role of a variable in a mathematical problem (dependent/independent/parameter)."""^^xs:string .

lbco:property a owl:Class ; #TOSEE: property is a keyword in OWL, add specifier?
    rdfs:label "Property" ;
    rdfs:comment "Observable property, such as a physical property of a material, the status of a device, etc"^^xs:string .

lbco:status a owl:Class ;
    rdfs:label "Status" ;
    rdfs:comment "For a single object or a system. E.g.: the software is installed/not-installed; the device is on/off, operational/faulty, the agent is available/busy, etc."^^xs:string ;
    rdfs:subClassOf lbco:property .

lbco:physical_quantity a owl:Class ;
     rdfs:label "Physical quantity" ;
     rdfs:subClassOf lbco:property .

lbco:annotation a owl:Class ;
    rdfs:label "Annotation" ;
    rdfs:comment "Annotation, analog to its use in the EVMPO, covers concepts other than the classes defined in this module"^^xs:string .

lbco:physical_object_status a owl:Class ;
    rdfs:label "Physical object status" ;
    rdfs:subClassOf lbco:status ;
    owl:oneOf (lbco:NEW lbco:OK lbco:BROKEN lbco:AGED) .

lbco:object_analysis_status a owl:Class ;
    rdfs:label "(Object) Analysis status" ;
    rdfs:subClassOf lbco:status ;
    owl:oneOf (lbco:ANALYZED lbco:NOT_ANALYZED) .

lbco:physical_object_network_status a owl:Class ;
    rdfs:label "Physical object network status" ;
    rdfs:subClassOf lbco:status ;
    owl:oneOf (lbco:ONLINE lbco:OFFLINE) .

lbco:process_status a owl:Class ;
    rdfs:label "Process status" ;
    rdfs:subClassOf lbco:status ;
    owl:oneOf (lbco:PLANNED lbco:RUNNING lbco:SUCCESS lbco:FAIL lbco:NEEDS_TO_BE_REDONE) .

lbco:agent_status a owl:Class ;
    rdfs:label "Agent status" ;
    rdfs:subClassOf lbco:status ;
    owl:oneOf (lbco:AVAILABLE lbco:BUSY lbco:NOT_AVAILABLE) .

lbco:batch a owl:Class ;
     rdfs:label "Batch" ;
     rdfs:comment "A batch is a set of experimental samples or products, acquired or manufactured/prepared together."^^xs:string ;
     rdfs:subClassOf lbco:physical_object ;
     rdfs:subClassOf [a owl:Restriction ;
          owl:onProperty lbco:has_part ;
          owl:someValuesFrom lbco:physical_object] .

### Object Properties ###

lbco:has_part a owl:ObjectProperty ;
    rdfs:label "has part" . 

lbco:has_participant a owl:ObjectProperty ;
    rdfs:domain lbco:process ;
    rdfs:range [owl:unionOf (lbco:agent lbco:physical_object lbco:software)] .

lbco:follows a owl:ObjectProperty ;
    rdfs:label "follows" ;
    rdfs:comment "refers to time ordering of process steps" ;
    rdfs:domain lbco:process_step ;
    rdfs:range lbco:process_step .

lbco:follows_directly a owl:ObjectProperty ;
    rdfs:label "follows directly" ;
    rdfs:comment "refers to time ordering of process steps" ;
    rdfs:domain lbco:process_step ;
    rdfs:range lbco:process_step ;
    rdfs:subPropertyOf lbco:follows .

lbco:has_input a owl:ObjectProperty ;
   rdfs:label "has input" ;
    rdfs:domain [owl:unionOf (lbco:process lbco:software lbco:agent lbco:device)] ;
    rdfs:range [owl:unionOf (lbco:data lbco:statement lbco:physical_object lbco:material lbco:property)] .
   
lbco:has_output a owl:ObjectProperty ;
   rdfs:label "has output" ;
   rdfs:domain [owl:unionOf (lbco:process lbco:software lbco:agent lbco:device)] ;
   rdfs:range [owl:unionOf (lbco:data lbco:statement lbco:physical_object lbco:material lbco:property)] .

lbco:is_output_of a owl:ObjectProperty ;  # TOSEE: necessary? 
                  owl:inverseOf lbco:has_output ;
   rdfs:range [owl:unionOf (lbco:process lbco:software lbco:agent lbco:device)] ;
   rdfs:domain [owl:unionOf (lbco:data lbco:statement lbco:physical_object lbco:material lbco:property)] .

lbco:has_setting a owl:ObjectProperty ;
   rdfs:label "has setting" ;
   rdfs:comment "Points to a property affecting the process"^^xs:string ;
   rdfs:comment "E.g., in a manufacturing process step, points to the tunable properties (settings)"^^xs:string ;
#   rdfs:comment "Settings, conditions affecting the process."^^xs:string ; # TODO: clarify how this is separate from "input" and how different from "constraint". E.g.: numerical parameters vs model parameters for a simulation
   rdfs:domain lbco:process ; # TOSEE: extend it to include also software+agent+device?
   rdfs:range lbco:property ; # TO SEE: extend it?
   rdfs:subPropertyOf lbco:has_property . # TO SEE: adapt based on the above

lbco:uses_method a owl:ObjectProperty ;
   rdfs:label "uses method" ;
   rdfs:domain lbco:process ;
   rdfs:range lbco:method .

lbco:answers a owl:ObjectProperty ;
    rdfs:domain lbco:statement ; 
    rdfs:range lbco:statement .
    
lbco:has_status a owl:ObjectProperty ;
    rdfs:domain [owl:unionOf (lbco:agent lbco:physical_object lbco:software lbco:process)] ;
    rdfs:range lbco:status ;
    rdfs:subPropertyOf lbco:has_property .

lbco:has_property a owl:ObjectProperty ; #TOSEE: property is a keyword in OWL, add specifier?
    rdfs:domain [owl:unionOf (lbco:agent lbco:physical_object lbco:software lbco:process)] ;
    rdfs:range lbco:property .

lbco:is_incompatible_with a owl:ObjectProperty ;
   rdfs:label "is incompatible with" ;
   rdfs:comment "relation between two entities that cannot meaningfully/correctly work together"^^xs:string ;
   rdfs:comment "E.g., it can be used to flag up incompatibilities (e.g., between materials, softwares, methods, etc.)"^^xs:string . 

lbco:is_compatible_with a owl:ObjectProperty ;
   rdfs:label "is compatible with" ;
   rdfs:comment "relation between two entities that can meaningfully/correctly work together"^^xs:string ;
   rdfs:comment "E.g., it can be used to point out compatibilities (e.g., between materials, softwares, methods, etc.)"^^xs:string . 

# FA_CRO_3
lbco:is_material_incompatible_with_material a owl:ObjectProperty ;
   rdfs:label "is material incompatible with material" ;
   rdfs:comment "To flag up between materials (e.g., mixtures that are unsafe, etc)"^^xs:string ;
   rdfs:domain lbco:material ; 
   rdfs:range lbco:material ;
   a owl:SymmetricProperty ;
   rdfs:subPropertyOf lbco:is_incompatible_with .
   
### Datatype Properties ###

lbco:has_batch_ID a owl:DatatypeProperty ;
    rdfs:label "has batch ID" ;
    rdfs:domain lbco:batch ;
    rdfs:range rdfs:Literal .

# see also: EMMO-LITE, DOLCE-LITE, EVMPO.

