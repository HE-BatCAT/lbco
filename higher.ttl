mbco:agent a owl:Class ;
    rdfs:label "Agent" ;
    owl:disjointUnionOf (mbco:human_agent mbco:machine_agent) .

mbco:human_agent a owl:Class ;
    rdfs:label "Human agent" .

mbco:machine_agent a owl:Class ;
    rdfs:label "Machine agent" .

mbco:data a owl:Class ;
    rdfs:label "Data" .

mbco:material a owl:Class ;
    rdfs:label "Material" .

mbco:process a owl:Class ;
    rdfs:label "Process" ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_input ;
                   owl:someValuesFrom owl:Thing] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_output ;
                   owl:someValuesFrom owl:Thing] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_participant ; # TODO: clarify how different from input/output (cf. BVCO)??? E.g.: a machine+operator+sample are participants, input is certain data, output is other data.
                   owl:someValuesFrom owl:Thing] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_setting ;
                   owl:someValuesFrom owl:Thing] .		   

mbco:process_step a owl:Class ;
    rdfs:label "Process step" ;
    rdfs:subClassOf mbco:process ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty [owl:inverseOf mbco:has_part] ;
                   owl:someValuesFrom mbco:process] .

mbco:software a owl:Class ;
    rdfs:label "Software" .

mbco:physical_object a owl:Class ;
    rdfs:label "(Physical) Object" . 

mbco:sample a owl:Class ;
    rdfs:label "Sample" ;
    rdfs:subClassOf mbco:physical_object .

mbco:device a owl:Class ;
    rdfs:label "Device" ;
    rdfs:subClassOf mbco:physical_object ;
    rdfs:comment "A physical object made for a given functionality"^^xs:string .

mbco:computational_device a owl:Class ;
    rdfs:label "Computational device" ;
    rdfs:subClassOf mbco:device ;
    rdfs:comment "A device that can perform a (digital) computational action."^^xs:string ;
    rdfs:comment "E.g., a PC, a computational cluster."^^xs:string .

mbco:characterization_device a owl:Class ;
    rdfs:label "Characterization device" ;
    rdfs:subClassOf mbco:device ;
    rdfs:comment "A device that can perform a characterization action."^^xs:string ;
    rdfs:comment "E.g., a potentiostat."^^xs:string .

mbco:manufacturing_device a owl:Class ;
    rdfs:label "Manufacturing device" ;
    rdfs:subClassOf mbco:device ;
    rdfs:comment "A device that can perform a manufacturing action"^^xs:string ;
    rdfs:comment "E.g., a mixer, a calenderer."^^xs:string .    

mbco:method a owl:Class ;
    rdfs:label "Method" ; 
    rdfs:comment "Could be a characterization method, a simulation method, a manufacturing method, etc."^^xs:string ;
    rdfs:subClassOf mbco:process . 

mbco:compute_resource a owl:Class ;
    rdfs:label "Compute resource" ; # we mean it as infrastrucure. Maybe should be renamed
    rdfs:comment "A physical or virtual machine on which software can be run."^^xs:string ;
    a owl:DeprecatedClass .

mbco:statement a owl:Class ;
    rdfs:label "Statement" . # Note: in EVMPO, statement is used for elementary communications. Here not, it is more general.   

mbco:role a owl:Class ;
    rdfs:label "Role" ;
    rdfs:comment "A non-rigid category (i.e., during its lifespan, an entity can change role)."^^xs:string ;
    rdfs:comment """For example: role of a human agent in an organization (employee/director), in a software repository (editor/owner); 
    role of a variable in a mathematical problem (dependent/independent/parameter)."""^^xs:string .

mbco:status a owl:Class ;
    rdfs:label "Status" ;
    rdfs:comment "For an single object or a system. E.g.: the software is installed/not-installed; the device is on/off, operational/faulty, the agent is available/busy, etc."^^xs:string .

mbco:annotation a owl:Class ;
    rdfs:label "Annotation" ;
    rdfs:comment "Annotation, analog to its use in the EVMPO, covers concepts other than the classes defined in this module"^^xs:string .

### Object Properties ###

mbco:has_part a owl:ObjectProperty ;
    rdfs:label "has part" . 

mbco:has_participant a owl:ObjectProperty ;
    rdfs:domain mbco:process ;
    rdfs:range [owl:unionOf (mbco:agent mbco:physical_object mbco:compute_resource mbco:software)] .

mbco:follows a owl:ObjectProperty ;
    rdfs:label "follows" ;
    rdfs:domain mbco:process_step ;
    rdfs:range mbco:process_step .

mbco:follows_directly a owl:ObjectProperty ;
    rdfs:label "follows directly" ;
    rdfs:domain mbco:process_step ;
    rdfs:range mbco:process_step ;
    rdfs:subPropertyOf mbco:follows .

mbco:has_input a owl:ObjectProperty ;
   rdfs:label "has input" .

mbco:has_output a owl:ObjectProperty ;
   rdfs:label "has output" .

mbco:has_setting a owl:ObjectProperty ;
   rdfs:label "has setting" ;
   rdfs:comment "Settings, conditions affecting the process."^^xs:string . # TODO: clarify how this is separate from "input" and how different from "constraint". E.g.: numerical parameters vs model parameters for a simulation

mbco:uses_method a owl:ObjectProperty ;
   rdfs:label "uses method" ;
   rdfs:range mbco:method .

mbco:answers a owl:ObjectProperty ;
    rdfs:domain mbco:statement ; 
    rdfs:range mbco:statement .
    
mbco:has_status a owl:ObjectProperty ;
    rdfs:domain [owl:unionOf (mbco:agent mbco:physical_object mbco:compute_resource mbco:software)] ;
    rdfs:range mbco:status .

