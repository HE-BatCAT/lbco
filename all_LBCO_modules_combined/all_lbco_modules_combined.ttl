	owl:onProperty lbco:observes ;
	owl:someValuesFrom lbco:vibration_frequency] . 

lbco:power_sensor a owl:Class ;
     rdfs:label "Power sensor" ;
     rdfs:subClassOf lbco:sensor ;
    rdfs:subClassOf [a owl:Restriction ;
	owl:onProperty lbco:observes ;
	owl:allValuesFrom lbco:power] .

lbco:power_monitoring_clip a owl:Class ;
     rdfs:label "Power monitoring clip" ;
     rdfs:subClassOf lbco:power_sensor .

lbco:gas_detection_sensor a owl:Class ;
     rdfs:label "Gas detection sensor" ;
     rdfs:subClassOf lbco:sensor ;
    rdfs:subClassOf [a owl:Restriction ;
	owl:onProperty lbco:observes ;
	owl:allValuesFrom lbco:gas_concentration] . 

lbco:carbon_monoxide_sensor a owl:Class ;
     rdfs:label "Carbon monoxide sensor" ;
