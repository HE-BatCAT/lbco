### Classes ###

# CAVEAT: Duplicated here from modelling.ttl for convenience
lbco:digital_twinning a owl:Class ; 
    rdfs:label "Digital twinning" ;
    rdfs:subClassOf lbco:computational_modelling_method .
lbco:computational_modelling_method rdfs:subClassOf lbco:method .    
# end of duplication

lbco:digital_twin_model a owl:Class ; 
    rdfs:label "Digital twin model" ;
    rdfs:subClassOf lbco:data ; # TOSEE: define a model class?
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:represents ;
                   owl:someValuesFrom owl:Thing] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_action_type ;
                   owl:someValuesFrom lbco:digital_twin_action] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_synchronization_rate ;
                   owl:someValuesFrom xs:decimal] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_generality_level ;
                   owl:someValuesFrom lbco:generality_level] .
		   
lbco:digital_twin_action a owl:Class ;
    rdfs:label "Digital twin action" ;
    owl:oneOf (lbco:VISUALIZE lbco:MONITOR lbco:PREDICT lbco:RECOMMEND) ;
    rdfs:comment """Based on an extract of application-related terms from ISO/IEC 30173:2023, 
    with an addition inspired by Barros et al. 2021, 'Requirements for an Ontology of Digital Twins'."""^^xs:string ;
    rdfs:subClassOf lbco:process .

lbco:generality_level a owl:Class ;
    rdfs:label "Generality level" ;
    owl:oneOf (lbco:UNIVERSAL lbco:TYPE lbco:INDIVIDUAL) ;
    rdfs:comment "Inspired by Barros et al. 2021, 'Requirements for an Ontology of Digital Twins'. Note: There a fourth level is present."^^xs:string ;
    rdfs:subClassOf lbco:annotation .

lbco:UNIVERSAL rdfs:comment "Encompassing, e.g. for a very generic entity or process."^^xs:string .
lbco:TYPE rdfs:comment "E.g., a car. Inspired by Barros et al. 2021, 'Requirements for an Ontology of Digital Twins'."^^xs:string .
lbco:INDIVIDUAL rdfs:comment "E.g., John's car. Inspired by Barros et al. 2021, 'Requirements for an Ontology of Digital Twins'."^^xs:string .

### Object Properties ###

lbco:represents a owl:ObjectProperty ;
    rdfs:label "represents" ;
    rdfs:range [owl:unionOf (lbco:physical_object lbco:process)] .

#lbco:has_action_type a owl:ObjectProperty ; # now defined in modelling.ttl
#    rdfs:label "has action type" .
#    rdfs:range lbco:digital_twin_action .

lbco:has_generality_level a owl:ObjectProperty ;
    rdfs:label "has generality level" ;
    rdfs:range lbco:generality_level .
    
### Datatype Properties ###

lbco:has_synchronization_rate a owl:DatatypeProperty ;
    rdfs:label "has synchronization rate" ;
    rdfs:comment """(Average) Rate (in 1/seconds) at which communication (in either direction) between 
    the digital entity and its target entity happens"""^^xs:string ;
    rdfs:domain lbco:digital_twin_model ;
    rdfs:range xs:decimal .

