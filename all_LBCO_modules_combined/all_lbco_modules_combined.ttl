     rdfs:subClassOf lbco:statement .

### Object Properties ###

lbco:has_product_design_parameter a owl:ObjectProperty ;
     rdfs:label "has product design parameter" ;
     rdfs:comment """points to a property that can be tuned in design phase.
     If it points to a material/object, it indicates a materials/object choice"""^^xs:string ;     
     rdfs:domain lbco:whole_product ;
     rdfs:range [owl:unionOf (lbco:property lbco:material lbco:physical_object)] .

lbco:has_product_kpi a owl:ObjectProperty ;
     rdfs:label "has product KPI" ;
     rdfs:comment "Points to a product Key Performance Indicator"^^xs:string ;
     rdfs:domain lbco:whole_product ;
     rdfs:range lbco:property ;
     rdfs:subPropertyOf lbco:has_property .
     
lbco:has_product_operating_condition a owl:ObjectProperty ;
     rdfs:label "has product operating condition" ;
     rdfs:domain lbco:whole_product ;
     rdfs:range lbco:property .

lbco:performs_action a owl:ObjectProperty ;
     rdfs:label "performs action" ;
     rdfs:domain lbco:actuator ;
     rdfs:range lbco:process . # TOSEE: define "action" class under "process" in higer.ttl?

lbco:has_process_kpi a owl:ObjectProperty ;
     rdfs:label "has process KPI" ;
     rdfs:domain lbco:process ;
     rdfs:range lbco:property ;
     rdfs:comment "Points to a process Key Performance Indicator"^^xs:string ;
     rdfs:subPropertyOf lbco:has_property .

lbco:observes a owl:ObjectProperty ;
     rdfs:label "observes" ;
     rdfs:domain lbco:sensor ;	
     rdfs:range lbco:physical_quantity ; # OK? it was: # lbco:property ;
     rdfs:comment "Inspired by W3C SSN/SOSA ontology for sensors"^^xs:string ;
     rdfs:subPropertyOf lbco:measures .

lbco:has_connection a owl:ObjectProperty ; # TO SEE: move to higher?
     rdfs:label "has connection" ;
     rdfs:domain lbco:physical_object ;
     rdfs:range lbco:connection .

lbco:has_trigger a owl:ObjectProperty ;
    rdfs:label "has trigger" ;
    rdfs:domain lbco:actuator ;
    rdfs:range lbco:quantitative_condition .

