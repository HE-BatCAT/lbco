     rdfs:subClassOf lbco:sensor ;
    rdfs:subClassOf [a owl:Restriction ;
	owl:onProperty lbco:observes ;
	owl:allValuesFrom lbco:acceleration] .

lbco:vibration_sensor a owl:Class ;
