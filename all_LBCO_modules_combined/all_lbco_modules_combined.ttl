<https://purl.org/lbco> dcterms:title "BatCAT core ontologies: module for Higher level concepts"^^xs:string ;
  owl:versionInfo "version 1.0.0"^^xs:string ;
  dcterms:modified "2025-12-15"^^xs:string ;
  dcterms:created "2025"^^xs:string ;
  dcterms:creator "Silvia Chiacchiera (ORCID 0000-0003-0422-7870)"^^xs:string ; # dcterms:creator <https://orcid.org/0000-0003-0422-7870> ;
  dcterms:contributor "Martin T. Horsch (ORCID 0000-0002-9464-6739)"^^xs:string ;
  dcterms:contributor "Gerhard Goldbeck (ORCID 0000-0002-4181-2852)"^^xs:string ;
