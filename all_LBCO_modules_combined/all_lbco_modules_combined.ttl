### Classes ###

lbco:dataset a owl:Class ;
    rdfs:label "Dataset" ;
    rdfs:subClassOf lbco:data .

lbco:data_mapping a owl:Class ;
    rdfs:label "Data mapping" ;
    rdfs:subClassOf lbco:annotation .

lbco:metadata_mapping a owl:Class ;
    rdfs:label "Metadata mapping" ;
    rdfs:subClassOf lbco:annotation .

lbco:metadata_scheme a owl:Class ;
    rdfs:label "Metadata scheme" ;
    rdfs:comment "Also called data model or schema"^^xs:string ;
    rdfs:subClassOf lbco:annotation .

lbco:file_format a owl:Class ;
    rdfs:label "File format" ;
    rdfs:subClassOf lbco:annotation .

lbco:data_quality_evaluation_method a owl:Class ;
    rdfs:label "Data quality evaluation method" ;
    rdfs:subClassOf lbco:method .

# From CQ_DAT_4
lbco:HUMAN_ASSESSMENT a lbco:data_quality_evaluation_method ;
    rdfs:label "Human assessment" .

lbco:QUALITY_INSURANCE_PROCEDURE a lbco:data_quality_evaluation_method ;
    rdfs:label "Quality insurance procedure" ;
    rdfs:comment "E.g., regular calibration checks."^^xs:string .

lbco:AUTOMATED_REPEATABILITY_TEST a lbco:data_quality_evaluation_method ;
    rdfs:label "Automated repeatability test" .

lbco:metadata_quality_evaluation_method a owl:Class ;
    rdfs:label "Metadata quality evaluation method" ;
    rdfs:subClassOf lbco:method .

lbco:SCANNING_BY_OOPS a lbco:metadata_quality_evaluation_method ;
    rdfs:comment "OOPS! Ontology Pitfall Scanner"^^xs:string ;
    rdfs:seeAlso "https://oops.linkeddata.es/index.jsp"^^xs:anyURI ;
    rdfs:comment """Poveda-Villalón, María; Gómez-Pérez, Asunción; and Suárez-Figueroa,
    Mari Carmen. "OOPS! (Ontology Pitfall Scanner!): An on-line tool for ontology evaluation."
    International Journal on Semantic Web and Information Systems (IJSWIS) 10.2 (2014): 7-34."""^^xs:string .

lbco:SCANNING_BY_FOOPS a lbco:metadata_quality_evaluation_method ;
		       a lbco:data_fairness_evaluation_method ;
    rdfs:comment "FOOPS! - Ontology Pitfall Scanner for FAIR"^^xs:string ;
    rdfs:seeAlso "https://foops.linkeddata.es/FAIR_validator.html"^^xs:anyURI ;
    rdfs:seeAlso "https://ceur-ws.org/Vol-2980/paper321.pdf"^^xs:anyURI .

lbco:data_fairness_evaluation_method a owl:Class ;
    rdfs:label "Data FAIRness evaluation method" ;
    rdfs:subClassOf lbco:method .

lbco:SCANNING_BY_FUJI a lbco:data_fairness_evaluation_method ;
    rdfs:comment "F-UJI - Automated FAIR Data Assessment Tool"^^xs:string ;
    rdfs:seeAlso "https://www.f-uji.net/"^^xs:anyURI ;
    rdfs:comment "https://doi.org/10.5281/zenodo.6461229"^^xs:string .

lbco:raw_data a owl:Class ;
    rdfs:label "Raw data" ;
    rdfs:subClassOf lbco:data ;
    rdfs:comment "Data as it is generated (e.g., from a software or device)"^^xs:string .

lbco:processed_data a owl:Class ;
    rdfs:label "Processed data" ;
    rdfs:subClassOf lbco:data ;
    rdfs:comment "At variance with raw data, processed data, after generation, was modified, either to clean, extract, transform it etc."^^xs:string .

lbco:data_structure a owl:Class ;
    rdfs:label "Data structure" ;
    rdfs:comment "E.g., includes data shapes, as SHACL or OO-LD shapes."^^xs:string ;
    rdfs:subClassOf lbco:annotation .

lbco:uncertainty_quantification_method a owl:Class ;
    rdfs:label "Uncertainty quantification method" ;
    rdfs:subClassOf lbco:method .

#lbco:BLOCK_AVERAGING a lbco:uncertainty_quantification_method ;
#    rdfs:label "Block averaging" .
    
lbco:ERROR_PROPAGATION a lbco:uncertainty_quantification_method ;
    rdfs:label "Error propagation" .

lbco:ERROR_ESTIMATE_VIA_STATISTICAL_METHODS a lbco:uncertainty_quantification_method ;
    rdfs:label "Error estimate via statistical methods" .

lbco:data_processing_method a owl:Class ;
    rdfs:label "Data processing method" ;
    rdfs:subClassOf lbco:method .

# from CQ_DAT_3.1
lbco:INCOMPLETE_DATASET_REMOVAL a lbco:data_processing_method ;
    rdfs:comment "Removal of datasets that are incomplete (w.r.t. some data structure)"^^xs:string .    

lbco:INCOMPLETE_DATASET_PADDING a lbco:data_processing_method ;
    rdfs:comment "Padding of datasets that are incomplete (w.r.t. some data structure) with dummy values"^^xs:string .    

# From RoMM and VISO
lbco:EXTRACTION a lbco:data_processing_method ;
     rdfs:comment """A method that enriches the dataset/state description by further information
                   calculated based on the information already contained in the state.
                   Examples include averaging, interpolation and the calculation of physical quantities (cf. RoMM)."""^^xs:string .

lbco:CONVERSION a lbco:data_processing_method ;
     rdfs:comment """A method that changes the data without altering the physical state itself
		(e.g., it changes the numerical representation of a state, cf. RoMM)."""^^xs:string .

### Object Properties ###

lbco:is_dataset_compatible_with_software a owl:ObjectProperty ;
    rdfs:label "is dataset compatible with software" ;
    rdfs:domain lbco:dataset ; # TOSEE: use data_format instead???
    rdfs:range lbco:software ;
    rdfs:subPropertyOf lbco:is_compatible_with .
    
lbco:complies_with a owl:ObjectProperty ;
    rdfs:domain lbco:dataset ; 
    rdfs:range lbco:data_structure . # TOSEE: add metadata_scheme to range?

lbco:is_generated_by a owl:ObjectProperty ; # TOSEE: use inverse (has_output) instead of defining this additional prop?
    rdfs:label "is generated by" ;
    rdfs:domain lbco:data ; 
    rdfs:range [owl:unionOf (lbco:agent lbco:device lbco:software)] ;
#    rdfs:subPropertyOf [owl:inverseOf lbco:has_output] .
    rdfs:subPropertyOf lbco:is_output_of .

lbco:has_data_processing_method a owl:ObjectProperty ;
    rdfs:label "has data processing method" ;
    rdfs:domain lbco:processed_data ;
    rdfs:range lbco:data_processing_method .

# CQ_DAT_8
lbco:is_annotated_using_scheme a owl:ObjectProperty ;
    rdfs:label "is_annotated_using_scheme" ;
    rdfs:domain lbco:data ;
    rdfs:range lbco:metadata_scheme .

