# FA_SIM_1
lbco:has_nn_input_dimensionality a owl:DatatypeProperty ;
    rdfs:label "has NN input dimensionality" ;
    rdfs:domain lbco:neural_network_model ;
    rdfs:range xs:nonNegativeInteger ;
    rdfs:comment "NN models (e.g., surrogate models) can be classified by the dimensionality of input and output fed to the NN."^^xs:string ;
    rdfs:comment "For example, input can be numbers, geometry/image, a transient"^^xs:string .

lbco:has_nn_output_dimensionality a owl:DatatypeProperty ;
     rdfs:label "has NN output dimensionality" ;
     rdfs:domain lbco:neural_network_model ;
     rdfs:range xs:nonNegativeInteger ;
     rdfs:comment "NN models (e.g., surrogate models) can be classified by the dimensionality of input and output fed to the NN."^^xs:string ;
     rdfs:comment "For example, output can be a scalar quantity, a local quantity or replace an equation"^^xs:string .
    
lbco:has_applicability_range_info a owl:DatatypeProperty ;
     rdfs:label "has_applicability_range_info" ;
     rdfs:domain lbco:model_parametrization ;
     rdfs:range xs:string ;
     rdfs:comment "Gives informations on the range (e.g., temperature) in which the model can be (meaningfully) applied"^^xs:string .

