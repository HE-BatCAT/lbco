### Classes ###

mbco:sample a owl:Class ;
     rdfs:label "Sample" ;
     rdfs:subClassOf mbco:physical_object ;
     rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty [owl:inverseOf mbco:has_part] ;
                   owl:someValuesFrom mbco:batch] .

mbco:sample_history a owl:Class ;
     rdfs:label "Sample history" ;
     rdfs:subClassOf mbco:data ;
     rdfs:comment """Track the whole history (across laboratories) of each experimental sample being 
characterized in BatCAT. It includes input and preparation conditions, records all processes undergone by a sample, 
including its artificial / accelerated ageing."""^^xs:string. 

mbco:characterization_technique a owl:Class ;
     rdfs:label "Characterization technique" ;
     rdfs:subClassOf mbco:method ;
     rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_participant ; 
                   owl:someValuesFrom mbco:characterization_device] ;
     rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_participant ; 
                   owl:someValuesFrom mbco:sample] ;
     rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_output ; 
                   owl:someValuesFrom mbco:physical_quantity] .

mbco:characterization_measurement a owl:Class ;
     rdfs:label "Characterization measurement" ;
     rdfs:subClassOf mbco:process .

mbco:sensor a owl:Class ; # TODO: connect to SSN/SOSA
     rdfs:label "Sensor" ;
     rdfs:subClassOf mbco:device .     

mbco:actuator a owl:Class ; # TODO: connect to SSN/SOSA. TOSEE: duplicated in Manufacturing module
     rdfs:label "Actuator" ;
     rdfs:subClassOf mbco:device .     

### Object Properties ###

mbco:has_sample_history a owl:ObjectProperty ;
     rdfs:label "has sample history" ;
     rdfs:domain mbco:sample ;
     rdfs:range mbco:sample_history .

### Datatype Properties ###

mbco:has_sample_ID a owl:DatatypeProperty ;
    rdfs:label "has sample ID" ;
    rdfs:domain mbco:sample .

