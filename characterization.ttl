### Classes ###

lbco:sample a owl:Class ;
     rdfs:label "Sample" ;
     rdfs:subClassOf lbco:physical_object ;
     rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty [owl:inverseOf lbco:has_part] ;
                   owl:someValuesFrom lbco:batch] .

lbco:sample_history a owl:Class ;
     rdfs:label "Sample history" ;
     rdfs:subClassOf lbco:data ;
     rdfs:comment """Track the whole history (across laboratories) of each experimental sample being 
characterized in BatCAT. It includes input and preparation conditions, records all processes undergone by a sample, 
including its artificial / accelerated ageing."""^^xs:string. 

lbco:characterization_technique a owl:Class ;
     rdfs:label "Characterization technique" ;
     rdfs:subClassOf lbco:method ;
     rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_participant ; 
                   owl:someValuesFrom lbco:characterization_device] ;
     rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_participant ; 
                   owl:someValuesFrom lbco:sample] ;
     rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:has_output ; 
                   owl:someValuesFrom lbco:physical_quantity] .

lbco:characterization_measurement a owl:Class ;
     rdfs:label "Characterization measurement" ;
     rdfs:subClassOf lbco:process ;
     rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:uses_method ; 
                   owl:someValuesFrom lbco:characterization_technique] .
     
lbco:characterization_device a owl:Class ;
     rdfs:label "Characterization device" ;
     rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty lbco:measures ; 
                   owl:someValuesFrom lbco:physical_quantity] .

lbco:sensor a owl:Class ; # TODO: connect to SSN/SOSA
     rdfs:label "Sensor" ;
     rdfs:subClassOf lbco:device .     

lbco:actuator a owl:Class ; # TODO: connect to SSN/SOSA. TOSEE: duplicated in Manufacturing module
     rdfs:label "Actuator" ;
     rdfs:subClassOf lbco:device .     

### Object Properties ###

lbco:has_sample_history a owl:ObjectProperty ;
     rdfs:label "has sample history" ;
     rdfs:domain lbco:sample ;
     rdfs:range lbco:sample_history .

lbco:measures a owl:ObjectProperty ; # TO SEE: connect to lbco:observes, used for sensors? Move it to higher?
     rdfs:label "measures" ;
     rdfs:domain lbco:device ;
     rdfs:range lbco:physical_quantity .

### Datatype Properties ###

lbco:has_sample_ID a owl:DatatypeProperty ;
    rdfs:label "has sample ID" ;
    rdfs:domain lbco:sample ;
    rdfs:range rdfs:Literal .

