### Classes ###

mbco:manufacturing_line a owl:Class ;
     rdfs:label "Manufacturing line" ;
     rdfs:subClassOf mbco:manufacturing_device .

mbco:whole_product a owl:Class ;
     rdfs:label "Whole product" ;
     rdfs:subClassOf mbco:physical_object .

mbco:intermediate_product a owl:Class ;
     rdfs:label "Intermediate product" ;
     rdfs:subClassOf mbco:physical_object .

mbco:product_component a owl:Class ;
     rdfs:label "Product component" ;
     rdfs:subClassOf mbco:physical_object .

# mbco:manufacturing_technique a owl:Class ;
#      rdfs:label "Manufacturing technique" ;
#      rdfs:subClassOf mbco:method .

mbco:sensor a owl:Class ; # TODO: connect to SSN/SOSA. TO SEE: duplicated in Characterization module
     rdfs:label "Sensor" ;
     rdfs:subClassOf mbco:device .

mbco:actuator a owl:Class ; # TODO: connect to SSN/SOSA. TO SEE: duplicated in Characterization module
     rdfs:label "Actuator" ;
     rdfs:subClassOf mbco:agent .

# sensors from CQ_MAN_3.2 (Thermocouples, Accelerometer + vibration sensor, Power monitoring clip, CO / CO2 sensor, Pressure sensor, Laser profilometer)
# TO SEE: 

mbco:temperature_sensor a owl:Class ;
     rdfs:label "Temperature sensor" ;
     rdfs:subClassOf mbco:sensor ;
     rdfs:subClassOf [a owl:Restriction ;
	owl:onProperty mbco:observes ;
	owl:allValuesFrom mbco:temperature] . # TOSEE: why is the reasoner not deducing that temperature is a mbco:property?

mbco:thermocouple a owl:Class ;
     rdfs:label "Thermocouple" ;
     rdfs:subClassOf mbco:temperature_sensor .

mbco:accelerometer a owl:Class ;
     rdfs:label "Accelerometer" ;
     rdfs:subClassOf mbco:sensor .
#     rdfs:subClassOf [a owl:Restriction ;
#	owl:onProperty mbco:observes ;
#	owl:allValuesFrom mbco:acceleration] .

mbco:vibration_sensor a owl:Class ;
     rdfs:label "Vibration sensor" ;
     rdfs:subClassOf mbco:sensor .
#     rdfs:subClassOf [a owl:Restriction ;
#	owl:onProperty mbco:observes ;
#	owl:allValuesFrom mbco:vibration] . # TOSEE: OK? is vibration a property?

mbco:power_sensor a owl:Class ;
     rdfs:label "Power sensor" ;
     rdfs:subClassOf mbco:sensor .
#     rdfs:subClassOf [a owl:Restriction ;
#	owl:onProperty mbco:observes ;
#	owl:allValuesFrom mbco:power] .

mbco:power_monitoring_clip a owl:Class ;
     rdfs:label "Power monitoring clip" ;
     rdfs:subClassOf mbco:power_sensor .

mbco:gas_detection_sensor a owl:Class ;
     rdfs:label "Gas detection sensor" ;
     rdfs:subClassOf mbco:sensor .
#     rdfs:subClassOf [a owl:Restriction ;
#	owl:onProperty mbco:observes ;
#	owl:allValuesFrom mbco:gas_density] . # TOSEE: OK?

mbco:carbon_monoxide_sensor a owl:Class ;
     rdfs:label "Carbon monoxide sensor" ;
     skos:altTabel "CO sensor" ;
     rdfs:subClassOf mbco:gas_detection_sensor .

mbco:carbon_dioxide_sensor a owl:Class ;
     rdfs:label "Carbon dioxide sensor" ;
     skos:altTabel "CO2 sensor" ;
     rdfs:subClassOf mbco:gas_detection_sensor .

mbco:pressure_sensor a owl:Class ;
     rdfs:label "Pressure sensor" ;
     rdfs:subClassOf mbco:sensor .
#     rdfs:subClassOf [a owl:Restriction ;
#	owl:onProperty mbco:observes ;
#	owl:allValuesFrom mbco:pressure] .

mbco:laser_profilometer a owl:Class ;
     rdfs:label "Laser profilometer" ;
     rdfs:subClassOf mbco:device ; 
     rdfs:subClassOf mbco:characterization_device ; # TOSEE: not a sensor, OK? Should "on-line charaterization" be defined?
     rdfs:comment "Can be used for on-line characterization (e.g., thickness measurement)"^^xs:string .
#     rdfs:subClassOf [a owl:Restriction ;
#	owl:onProperty mbco:observes ;
#	owl:someValuesFrom mbco:thickness] .

# CQ_MAN_3
mbco:connection a owl:Class ;
     rdfs:label "Connection" ;
     owl:oneOf (mbco:WIRED_ELECTRICAL_CABLE mbco:BLUETOOTH mbco:OPTICAL_FIBRE) .

### Object Properties ###

mbco:has_product_design_parameter a owl:ObjectProperty ;
     rdfs:label "has product design parameter" ;
     rdfs:domain mbco:whole_product .

mbco:has_product_kpi a owl:ObjectProperty ;
     rdfs:label "has product KPI" ;
     rdfs:comment "Points to a product Key Performance Indicator"^^xs:string ;
     rdfs:domain mbco:whole_product .
     
mbco:has_product_operating_condition a owl:ObjectProperty ;
     rdfs:label "has product operating condition" ;
     rdfs:domain mbco:whole_product .

mbco:performs_action a owl:ObjectProperty ;
     rdfs:label "performs action" ;
     rdfs:domain mbco:actuator ;
     rdfs:range mbco:process . # TOSEE: define "action" class under "process" in higer.ttl?

mbco:has_process_kpi a owl:ObjectProperty ;
     rdfs:label "has process KPI" ;
     rdfs:domain mbco:process ;	
     rdfs:comment "Points to a process Key Performance Indicator"^^xs:string .

mbco:observes a owl:ObjectProperty ;
     rdfs:label "observes" ;
     rdfs:range mbco:property ;	
     rdfs:comment "Inspired by W3C SSN/SOSA ontology for sensors"^^xs:string .

### Datatype Properties ###

mbco:has_trigger a owl:DatatypeProperty ;
    rdfs:label "has trigger" ;
    rdfs:domain mbco:actuator .

### Restrictions ###

mbco:whole_product
	rdfs:subClassOf [a owl:Restriction ;
	owl:onProperty mbco:has_part ;
	owl:someValuesFrom mbco:product_component] ;
	rdfs:subClassOf [a owl:Restriction ;
	owl:onProperty mbco:has_part ;
	owl:someValuesFrom mbco:material] .

mbco:sensor
	rdfs:subClassOf [a owl:Restriction ;
	owl:onProperty mbco:observes ;
	owl:allValuesFrom mbco:property] ; # TOSEE: property -> physical_property? 
	rdfs:subClassOf [a owl:Restriction ;
	owl:onProperty mbco:has_connection ;
	owl:allValuesFrom mbco:connection] .

