# CQ_MAN_10
mbco:personnel_unit_cost a owl:Class ;
        rdfs:label "Personnel unit cost" ;
        rdfs:comment "Personnel cost, e.g., per person per hour"^^xs:string ;
	rdfs:subClassOf mbco:property .

mbco:electricity_unit_cost a owl:Class ;
        rdfs:label "Electricity unit cost" ;
        rdfs:comment "E.g., cost per kWh"^^xs:string ;
	rdfs:subClassOf mbco:property .

mbco:production_cost_per_unit a owl:Class ; # NB: duplicated here from battery module
     rdfs:label "Production cost per unit" ;
     rdfs:subClassOf mbco:property . 

