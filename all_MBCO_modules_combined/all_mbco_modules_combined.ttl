### Classes ###

mbco:dataset a owl:Class ;
    rdfs:label "Dataset" ;
    rdfs:subClassOf mbco:data .

mbco:data_mapping a owl:Class ;
    rdfs:label "Data mapping" .

mbco:metadata_mapping a owl:Class ;
    rdfs:label "Metadata mapping" .

mbco:metadata_scheme a owl:Class ;
    rdfs:label "Metadata scheme" ;
    rdfs:comment "Also called data model or schema"^^xs:string .

mbco:file_format a owl:Class ;
    rdfs:label "File format" .

mbco:data_quality_evaluation_method a owl:Class ;
    rdfs:label "Data quality evaluation method" ;
    rdfs:subClassOf mbco:method .

# From CQ_DAT_4
mbco:HUMAN_ASSESSMENT a mbco:data_quality_evaluation_method ;
    rdfs:label "Human assessment" .

mbco:QUALITY_INSURANCE_PROCEDURE a mbco:data_quality_evaluation_method ;
    rdfs:label "Quality insurance procedure" ;
    rdfs:comment "E.g., regular calibration checks."^^xs:string .

mbco:AUTOMATED_REPEATABILITY_TEST a mbco:data_quality_evaluation_method ;
    rdfs:label "Automated repeatability test" .

mbco:metadata_quality_evaluation_method a owl:Class ;
    rdfs:label "Metadata quality evaluation method" ;
    rdfs:subClassOf mbco:method .

mbco:raw_data a owl:Class ;
    rdfs:label "Raw data" ;
    rdfs:subClassOf mbco:data ;
    rdfs:comment "Data as it is generated (e.g., from a software or device)"^^xs:string .

mbco:processed_data a owl:Class ;
    rdfs:label "Processed data" ;
    rdfs:subClassOf mbco:data ;
    rdfs:comment "At variance with raw data, processed data, after generation, was modified, either to clean, extract, transform it etc."^^xs:string .

mbco:data_structure a owl:Class ;
    rdfs:label "Data structure" ;
    rdfs:comment "E.g., includes data shapes, as SHACL or OO-LD shapes."^^xs:string .    

mbco:uncertainty_quantification_method a owl:Class ;
    rdfs:label "Uncertainty quantification method" ;
    rdfs:subClassOf mbco:method .

#mbco:BLOCK_AVERAGING a mbco:uncertainty_quantification_method ;
#    rdfs:label "Block averaging" .
    
mbco:ERROR_PROPAGATION a mbco:uncertainty_quantification_method ;
    rdfs:label "Error propagation" .

mbco:ERROR_ESTIMATE_VIA_STATISTICAL_METHODS a mbco:uncertainty_quantification_method ;
    rdfs:label "Error estimate via statistical methods" .

mbco:data_processing_method a owl:Class ;
    rdfs:label "Data processing method" ;
    rdfs:subClassOf mbco:method .

# from CQ_DAT_3.1
mbco:INCOMPLETE_DATASET_REMOVAL a mbco:data_processing_method ;
    rdfs:comment "Removal of datasets that are incomplete (w.r.t. some data structure)"^^xs:string .    

mbco:INCOMPLETE_DATASET_PADDING a mbco:data_processing_method ;
    rdfs:comment "Padding of datasets that are incomplete (w.r.t. some data structure) with dummy values"^^xs:string .    

# From RoMM and VISO
mbco:EXTRACTION a mbco:data_processing_method ;
     rdfs:comment """A method that enriches the dataset/state description by further information
                   calculated based on the information already contained in the state.
                   Examples include averaging, interpolation and the calculation of physical quantities (cf. RoMM)."""^^xs:string .

mbco:CONVERSION a mbco:data_processing_method ;
     rdfs:comment """A method that changes the data without altering the physical state itself
		(e.g., it changes the numerical representation of a state, cf. RoMM)."""^^xs:string .

### Object Properties ###

mbco:is_compatible_with a owl:ObjectProperty ;
    rdfs:label "is compatible with" ;
    rdfs:domain mbco:dataset ; # TOSEE: use data_format instead???
    rdfs:range mbco:software .
    
mbco:complies_with a owl:ObjectProperty ;
    rdfs:domain mbco:dataset ; 
    rdfs:range mbco:data_structure . # TOSEE: add metadata_scheme to range?

mbco:is_generated_by a owl:ObjectProperty ; # TOSEE: use inverse (has_output) instead of defining this additional prop?
    rdfs:label "is generated by" ;
    rdfs:domain mbco:data ; 
    rdfs:range [owl:unionOf (mbco:agent mbco:device mbco:software)] .

mbco:has_data_processing_method a owl:ObjectProperty ;
    rdfs:label "has data processing method" ;
    rdfs:domain mbco:processed_data ;
    rdfs:range mbco:data_processing_method .

# CQ_DAT_8
mbco:is_annotated_using_scheme a owl:ObjectProperty ;
    rdfs:label "is_annotated_using_scheme" ;
    rdfs:domain mbco:data ;
    rdfs:range mbco:metadata_scheme .

### Datatype Properties ###

mbco:has_average a owl:DatatypeProperty ;
    rdfs:label "has average" ;
    rdfs:domain mbco:data ;
    rdfs:comment "Only applicable to a homogeneous set of data (i.e., data with same units)"^^xs:string .

mbco:has_error a owl:DatatypeProperty ;
    rdfs:label "has error" ;
    rdfs:domain mbco:data .

# Should/could add:
# systematic error
# statistical error
# random error
# total? error
# mbco:STANDARD_DEVIATION a mbco:error_type .
# mbco:STANDARD_ERROR a mbco:error_type ;
#    rdfs:comment "Error calculation based on standard deviation and the assumption of independent data points."^^xs:string .

mbco:has_data_processing_info a owl:DatatypeProperty ;
    rdfs:label "has data processing info" ;
    rdfs:domain mbco:processed_data ;
    rdfs:range xs:string .

# CQ_DAT_1
mbco:is_complete_dataset a owl:DatatypeProperty ;
    rdfs:label "is_complete_dataset" ;
    rdfs:domain mbco:dataset ;
    rdfs:range xs:boolean ;
    rdfs:comment """Given a dataset that complies with a data structure, specifies whether all fields are given (complete) or some are missing
    (incomplete)"""^^xs:string .

# CQ_DAT_2
mbco:contains_dummy_values a owl:DatatypeProperty ;
    rdfs:label "contains dummy values" ;
    rdfs:domain mbco:dataset ;
    rdfs:range xs:boolean .

