mbco:manufacturing_process 
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_process_kpi ;
	owl:someValuesFrom mbco:energy_consumption] ;
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_process_kpi ;
	owl:someValuesFrom mbco:equipment_performance] ;
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_process_kpi ;
	owl:someValuesFrom mbco:waste_produced] ; 
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_process_kpi ;
	owl:someValuesFrom mbco:product_quality] .

mbco:manufacturing_process 
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_participant ;
	owl:someValuesFrom mbco:manufacturing_device] ;
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_input ;
	owl:someValuesFrom [owl:unionOf (mbco:material mbco:physical_object)]] ;
	rdfs:subClassOf [a owl:Restriction ;
        owl:onProperty mbco:has_output ;
	owl:someValuesFrom mbco:physical_object] .

mbco:equipment_performance a owl:Class ;
	rdfs:subClassOf mbco:property .

mbco:product_quality a owl:Class ;
	rdfs:subClassOf mbco:property .

mbco:waste_produced a owl:Class ;
	rdfs:subClassOf mbco:property .

mbco:energy_consumption a owl:Class ;
	rdfs:subClassOf mbco:property .

