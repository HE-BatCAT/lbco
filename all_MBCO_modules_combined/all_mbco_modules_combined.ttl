  owl:versionInfo "version 0.1.8"^^xs:string ;
  dcterms:modified "2025-11-14"^^xs:string ;
  dcterms:creator "Silvia Chiacchiera (ORCID 0000-0003-0422-7870)"^^xs:string ;
