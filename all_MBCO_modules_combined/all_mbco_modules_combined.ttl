### Classes ###

# CAVEAT: Duplicated here from modelling.ttl for convenience
mbco:digital_twinning a owl:Class ; 
    rdfs:label "Digital twinning" ;
    rdfs:subClassOf mbco:computational_modelling_method .
mbco:computational_modelling_method rdfs:subClassOf mbco:method .    
# end of duplication

mbco:digital_twin_model a owl:Class ; 
    rdfs:label "Digital twin model" ;
    rdfs:subClassOf mbco:data ; # TOSEE: define a model class?
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:represents ;
                   owl:someValuesFrom owl:Thing] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_action_type ;
                   owl:someValuesFrom mbco:digital_twin_action] ;
    rdfs:subClassOf [a owl:Restriction ;
                   owl:onProperty mbco:has_synchronization_rate ;
                   owl:someValuesFrom xs:decimal] .

mbco:digital_twin_action a owl:Class ;
    rdfs:label "Digital twin action" ;
    owl:oneOf (mbco:VISUALIZE mbco:MONITOR mbco:PREDICT mbco:RECOMMEND) ;
    rdfs:comment """Based on an extract of application-related terms from ISO/IEC 30173:2023, 
    with an addition inspired by Barros et al. 2021, 'Requirements for an Ontology of Digital Twins'."""^^xs:string ;
    rdfs:subClassOf mbco:process .

mbco:generality_level a owl:Class ; 
    owl:oneOf (mbco:UNIVERSAL mbco:TYPE mbco:INDIVIDUAL) ;
    rdfs:comment "Inspired by Barros et al. 2021, 'Requirements for an Ontology of Digital Twins'. Note: There a fourth level is present."^^xs:string ;
    rdfs:subClassOf mbco:annotation .

mbco:UNIVERSAL rdfs:comment "Encompassing, e.g. for a very generic entity or process."^^xs:string .
mbco:TYPE rdfs:comment "E.g., a car. Inspired by Barros et al. 2021, 'Requirements for an Ontology of Digital Twins'."^^xs:string .
mbco:INDIVIDUAL rdfs:comment "E.g., John's car. Inspired by Barros et al. 2021, 'Requirements for an Ontology of Digital Twins'."^^xs:string .

### Object Properties ###

mbco:represents a owl:ObjectProperty ;
    rdfs:label "represents" ;
    rdfs:range [owl:unionOf (mbco:physical_object mbco:process)] .

mbco:has_action_type a owl:ObjectProperty ;
    rdfs:label "has action type" ;
    rdfs:range mbco:digital_twin_action .

mbco:has_generality_level a owl:ObjectProperty ;
    rdfs:label "has generality level" ;
    rdfs:range mbco:generality_level .
    
### Datatype Properties ###

mbco:has_synchronization_rate a owl:DatatypeProperty ;
    rdfs:label "has synchronization rate" ;
    rdfs:comment """(Average) Rate (in 1/seconds) at which communication (in either direction) between 
    the digital entity and its target entity happens"""^^xs:string .

