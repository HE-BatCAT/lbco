ex:GITT_TEST a lbco:method ;
    lbco:has_output ex:DIFFUSION_COEFFICIENT ;
    lbco:has_output ex:ACTIVE_MATERIAL_POTENTIALS ;
    lbco:has_participant ex:POTENTIOSTAT ;
    lbco:has_participant [a lbco:sample] ;
    lbco:has_participant [a lbco:agent] ;
    lbco:has_cost_level lbco:HIGH ;
    lbco:has_precision_level lbco:HIGH .       

ex:DISSIPATIVE_PARTICLE_DYNAMICS a lbco:method ;
    lbco:has_input ex:DPD_MODEL_PARAMETRIZATION_1 ;
    lbco:has_output ex:DIFFUSION_COEFFICIENT ;
    lbco:has_participant [a lbco:computational_device] ;
    lbco:has_participant [a lbco:software] ; # E.g., license access needed
    lbco:has_cost_level lbco:MEDIUM ;
    lbco:has_precision_level lbco:LOW .       
#    lbco:has_property ex:DPD_COST, ex:DPD_PRECISION .

ex:POTENTIOSTAT a lbco:characterization_device .
ex:DIFFUSION_COEFFICIENT a lbco:continuum_variable ;
			 a lbco:physical_quantity .
ex:ACTIVE_MATERIAL_POTENTIALS a lbco:continuum_variable ;
			 a lbco:physical_quantity .
ex:DPD_MODEL_PARAMETRIZATION_1 a lbco:model_parametrization . 

ex:DPD_DATA_ANALYSIS a lbco:process ;
    lbco:has_participant [a lbco:agent] .		   

ex:DPD_SIMULATION_SETTING_UP a lbco:process ;
    lbco:has_participant [a lbco:agent] .

ex:EXPERIMENT_SETTING_UP a lbco:process ;
    lbco:has_participant [a lbco:agent] .		   

ex:RESOURCE_ALLOCATION_EXAMPLE_PROBLEM
    a lbco:logic_based_problem ;
    a lbco:optimization_problem ;
    rdfs:comment "An example about running simulations"^^xs:string ;
    lbco:has_part ex:GOAL_1 ;
    lbco:has_part ex:TARGET_2 ;
    lbco:has_part ex:CONSTRAINT_1 ;
    lbco:has_part ex:CONSTRAINT_2 ;
    lbco:has_part ex:CONSTRAINT_3 ;
    lbco:has_part ex:INITIAL_STATUS_1 .

ex:GOAL_1 a lbco:goal ;
   rdfs:comment "Allocate human and computational devices to run 2 DPD simulations, respecting the ordering of steps."^^xs:string .

ex:INITIAL_STATUS_1 a lbco:initial_status ;
   rdfs:comment "Human and computational devices are available and operational. Needed software is installed"^^xs:string .

ex:TARGET_2 a lbco:target ;
   rdfs:comment "Maximize usage of resources (minimize their idle time)."^^xs:string .

ex:CONSTRAINT_1 a lbco:quantitative_constraint ;
    rdfs:comment "There is only one available agent"^^xs:string .

ex:CONSTRAINT_2 a lbco:quantitative_constraint ;
    rdfs:comment "There are two available computational devices"^^xs:string .

ex:CONSTRAINT_3 a lbco:qualitative_constraint ;
    rdfs:comment "A computational method must be used"^^xs:string .

ex:DPD_SIMULATION_RUN 
    lbco:uses_method ex:DISSIPATIVE_PARTICLE_DYNAMICS ;
    lbco:followsdirectly ex:DPD_SIMULATION_SETTING_UP .

ex:DPD_DATA_ANALYSIS lbco:followsdirectly ex:DPD_SIMULATION_RUN .

ex:COMPARISON_EXAMPLE_PROBLEM
    a lbco:logic_based_problem ;
    a lbco:optimization_problem ;
    rdfs:comment "An example about comparing simulations or experiments"^^xs:string ;
    lbco:has_part ex:GOAL_2 ;
    lbco:has_part ex:TARGET_1 .

ex:GOAL_2 a lbco:qualitative_constraint ;
    rdfs:comment "Evaluate the diffusion coefficient of a substance"^^xs:string ;
    lbco:has_part ex:DIFFUSION_COEFFICIENT .

ex:TARGET_1 a lbco:target ;
   rdfs:comment "Maximize precision and minimize cost"^^xs:string .

ex:SOLUTION_COMPARISON a lbco:solution ;
   lbco:answers ex:COMPARISON_EXAMPLE_PROBLEM ;
   lbco:uses_method [a lbco:answer_set_programming] .

ex:SOLUTION_RESOURCE_ALLOCATION a lbco:solution ;
   lbco:answers ex:RESOURCE_ALLOCATION_EXAMPLE_PROBLEM ;
   lbco:uses_method [a lbco:answer_set_programming] .

